<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Dashboard</title>
    <link rel="stylesheet" href="/css/styles.css">
</head>

<body>
    <h2>Welcome, <%= user.email %>
    </h2>
    <h3>Users:</h3>
    <ul>
        <% users.forEach(user=> { %>
            <li>
                <a href="/chat/<%= user._id %>">
                    <%= user.email %>
                </a>
            </li>
            <% }); %>
    </ul>
    <a href="/logout">Logout</a>
</body>

</html>